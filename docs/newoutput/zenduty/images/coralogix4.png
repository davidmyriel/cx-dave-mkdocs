<script>
document.addEventListener("DOMContentLoaded", function () {
   var headings = document.querySelectorAll(".article-content h1, .article-content h2, .article-content h3");

  headings.forEach(function (heading) {
    var headingId = heading.id;

    if (headingId) {
      var anchor = document.createElement("a");
      var anchorIcon = document.createElement("span");
      anchorIcon.textContent = "#";
      anchor.href = "#" + headingId;

      heading.classList.add("heading-link");
      anchor.classList.add("anchor-link");
      anchorIcon.classList.add("anchor-icon");

      anchor.appendChild(anchorIcon); 

      while (heading.firstChild) {
        anchor.appendChild(heading.firstChild);
      }

      heading.appendChild(anchor);
    }
  });
});
</script>

